{
  "name": "react-children-addons",
  "version": "1.0.8",
  "description": "React Children Addons",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/birkir/react-children-addons",
  "license": "MIT",
  "keywords": [
    "react",
    "children",
    "addons",
    "utils"
  ],
  "scripts": {
    "build": "babel src --out-dir ./lib --extensions '.ts' && tsc -p .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir ./lib"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-is": "^16.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.1.2",
    "@types/node": "10.12.0",
    "@types/react": "16.4.18",
    "@types/react-is": "16.5.0",
    "cross-env": "5.2.0"
  }
}
